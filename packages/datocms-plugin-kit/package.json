{
  "name": "@oddcommon/datocms-plugin-kit",
  "version": "0.0.1",
  "description": "A toolkit for building organized, modular DatoCMS plugins",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --max-warnings 0",
    "format": "prettier . --write",
    "precommit": "lint-staged",
    "clean": "rm -rf dist"
  },
  "author": "Alban Creton - OddCommon",
  "license": "MIT",
  "dependencies": {
    "datocms-plugin-sdk": "^2.0.15",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@internals/eslint-config": "workspace:*",
    "@internals/typescript-config": "workspace:*",
    "@types/node": "^24",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "eslint": "^9",
    "jsdom": "^27.1.0",
    "lint-staged": "^16",
    "tsdown": "^0.16",
    "typescript": "5",
    "vitest": "^4"
  },
  "engines": {
    "node": ">=22"
  }
}
